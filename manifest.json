{
    "name": "Extension Demo",
    "version": "1.0",
    "description": "Build an Basic Extension!",
    "permissions": ["activeTab","tabs","<all_urls>"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png"
      }
    },
    "manifest_version": 2
  }